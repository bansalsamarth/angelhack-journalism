

<!DOCTYPE html>
<html>
    <!--[if IE 9 ]><html class="ie9"><![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>PathGL</title>
    
        <!-- Vendor CSS -->
        <link href="/alpha/vendors/animate-css/animate.min.css" rel="stylesheet">
        <link href="/alpha/vendors/sweet-alert/sweet-alert.min.css" rel="stylesheet">
        <link href="/alpha/vendors/material-icons/material-design-iconic-font.min.css" rel="stylesheet">
        <link href="/alpha/vendors/socicon/socicon.min.css" rel="stylesheet">
            
        <!-- CSS -->
        <link href="/alpha/css/app.min.1.css" rel="stylesheet">
        <link href="/alpha/css/app.min.2.css" rel="stylesheet">
          <link rel="stylesheet" href="/pathgl/lib/adnan.css">


      

    </head>
    
    <body>
          <%- include ../partials/aheader.ejs %>
        
            <section id="content">
                <div class="container">                    

                    <div class="col-sm-12" style="text-align:center">
                        <!-- Font Size -->
                        <div class="card">
                            <canvas width="800" height="500"></canvas>
                                <input type = "range" id="slid" min = -500 max = 2500 value = -500 style="width:800px;">
                                <div id="year"></div>
                        </div>
                        <div class="card">
                              <div class="lc-block toggled" id="l-login">
                                <%- include ../partials/aflash.ejs %>
                                    login
                                </div>
                                
                                <!-- Register -->
                                <div class="lc-block" id="l-register">
                                            register
                                </div>
                        </div>
                        <div class="card">
                            <div class="card-header ch-alt">
                                <h2><b>Visualize your excel.</b></h2>
                            </div>

                             <div class="card-body card-padding">
                            <form id="uploadForm" enctype="multipart/form-data" method="post" action="/upload">
                                <!-- fieldsets -->
                                <fieldset>
                                    <h2 class="fs-title">Upload the excel</h2>
                                    <!-- <h3 class="fs-subtitle">This is step 1</h3> -->
                                    <input id = "file" type="file" name="file" />
                                    <input id ="_csrf" type="hidden" name="_csrf" value="<%=_csrf%>">
                                        <input id="button" type="button" />
                                </fieldset>
                            </form>
                                    </div>
                        </div>

                        
                        
                    </div>

                    
                </div>
            </section>
        

          <%- include ../partials/afooter.ejs %>
        <script src="/alpha/js/jquery-2.1.1.min.js"></script>
         <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
        <script src="/alpha/js/bootstrap.min.js"></script>   
        <script src="/pathgl/lib/d3.js"></script>
        <script src="/pathgl/lib/topojson.v1.min.js"></script>
        <script src="/pathgl/lib/projection.js"></script>
        <script src="/pathgl/lib/tip.js"></script>
        <script src="/pathgl/lib/heatmap.js"></script>
         <script src="/pathgl/code/js/map.js"></script>
    
    <script src="/pathgl/lib/pathgl.js"></script>
    </body>
</html>